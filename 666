glass1=glass2
glass2=temp

完整

glass1=input("请输入杯子1中的饮料")
glass2=input("请输入杯子2中的饮料")
temp=""#临时的杯子空纸被杯

print("交换前杯子1杯子2中的饮料")#交换前
print("glass1")#先打印杯子1中的数据
print("glass2")#打印杯子2中的数据

temp=glass1
glass1=glass2
glass2=temp

print("交换后杯子1杯子2中的饮料")#交换后
print("glass1")#先打印杯子1中的数据
print("glass2")#打印杯子2中的数据